<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <!-- interface-requires addnewdownload_dialog 1.0 -->
  <object class="AddnewdownloadDialog" id="addnewdownload_dialog">
